{
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "game" : {
    "title": "My first actual quiz",
    "meta": {
      "description": "Sample description",
      "tips": [
        "Tip 1",
        "Tip 2"
      ]
    },
    "quiz": [
      {
        "question": "Who sang the title song for the latest Bond film, No Time to Die?",
        "options": [
          "Adele",
          "Sam Smith",
          "Billie Eilish",
          "Sir Paul"
        ],
        "answer": "Billie Eilish"
      },
      {
        "question": "What company makes the Xperia model of smartphone?",
        "options": [
          "Samsung",
          "Sony",
          "Nokia"
        ],
        "answer": "Sony"
      },
      {
        "question": "Where was the first example of paper money used?",
        "options": [
          "China",
          "Turkey",
          "Greece",
          "Belarus"
        ],
        "answer": "China"
      },
      {
        "question": "If you were looking at Iguazu Falls, on what continent would you be?",
        "options": [
          "Asia",
          "Africa",
          "South America",
          "Europe"
        ],
        "answer": "South America"
      }
    ]
  },
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "release": "bumpp",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.6",
    "@zxing/library": "^0.20.0",
    "delaunator": "^5.0.0",
    "perfect-debounce": "^1.0.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/ic": "^1.1.14",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/ri": "^1.1.12",
    "@iconify-json/twemoji": "^1.1.12",
    "@nuxt/devtools": "npm:@nuxt/devtools-edge@1.0.2-28328457.999b2a1",
    "@nuxtjs/color-mode": "^3.3.0",
    "@types/delaunator": "^5.0.2",
    "@types/seedrandom": "^3.0.8",
    "@unocss/eslint-config": "^0.57.3",
    "@unocss/nuxt": "^0.57.3",
    "@vueuse/nuxt": "^10.6.0",
    "bumpp": "^9.2.0",
    "consola": "^3.2.3",
    "eslint": "^8.53.0",
    "floating-vue": "2.0.0-beta.24",
    "nuxt": "^3.8.1",
    "pinia": "^2.1.7",
    "qr-scanner-wechat": "^0.1.3",
    "simple-git": "^3.20.0",
    "stackblur-canvas": "^2.6.0",
    "typescript": "^5.2.2",
    "uqr": "^0.1.2",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.22"
  }
}
